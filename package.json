        "@babel/plugin-transform-modules-commonjs": "7.16.8",
        "@babel/preset-typescript": "7.16.7",
        "@babel/register": "7.16.9",
        "@types/archiver": "5.3.1",
        "@types/babel__core": "7.1.18",
        "@types/node-fetch": "2.5.12",
        "ts-node": "10.4.0",
        "typescript": "4.5.4"
    }
